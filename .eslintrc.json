{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": [
    "standard",
    "eslint:recommended"
  ],
  "plugins": [
    "babel"
  ],
  "rules": {
    "eqeqeq": [
      "error",
      "always",
      {
        "null": "ignore"
      }
    ],
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
